<?xml version="1.0" encoding="utf-8"?>
<layer-list  xmlns:android="http://schemas.android.com/apk/res/android">
<item>
<shape
  android:shape="ring" android:useLevel="false" android:innerRadius="0dip"
  android:thickness="14dip">
  <size android:width="32px" android:height="32px" />
  <gradient android:type="sweep" android:useLevel="false"
    android:startColor="#cccccc" android:centerColor="#999999"
    android:endColor="#cccccc" />
  <stroke android:width="2dip" android:color="#33666666" />
</shape>
</item>
<item>
<shape
  android:shape="ring" android:useLevel="false" android:innerRadius="0dip"
  android:thickness="10dip">
  <size android:width="32px" android:height="32px" />
  <gradient android:type="radial" android:useLevel="false" android:gradientRadius="20"
    android:startColor="#cccccc" android:centerColor="#ffffff"
    android:endColor="#999999" android:centerY="1"/>
  <stroke android:width="1dip" android:color="#ffffff" />
</shape>
</item>
</layer-list>